#include<stdio.h>
#include<math.h>
int main()
{
int num, rev=0, a,remainder;
printf(" enter the number to find palindrome or not \n");
scanf("%d\n",&num);
num=a;
while(num>0)
{
remainder=num%10;
rev=rev*10+remainder;
num=num/10;
}
if(rev==a)
printf(" the number is palindrome \n");
else
printf(" the number is not a palindrome \n");
return 0;
}
